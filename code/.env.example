# 새만금개발청 공공데이터 API 설정
# Saemangeum Development Agency Public Data API Configuration

# 카카오맵 JavaScript API 키 (developers.kakao.com에서 발급)
VITE_KAKAO_MAP_API_KEY=your_kakao_javascript_api_key_here

# API 기본 설정
VITE_API_BASE_URL=https://api.data.go.kr/openapi/tn_pubr_public_saemangeum_api

# API 인증키 (data.go.kr에서 발급)
VITE_API_SERVICE_KEY=your_api_service_key_here

# API 요청 설정

# 카카오맵 API 설정
VITE_KAKAO_MAP_APP_KEY=your_kakao_map_app_key_here 
VITE_API_TIMEOUT=30000
VITE_API_RETRY_COUNT=3
VITE_CACHE_DURATION=300000

# 개발 환경 설정
VITE_APP_ENV=development
VITE_DEBUG_MODE=true

# 데이터 품질 임계값
VITE_DATA_QUALITY_THRESHOLD=80

# 새만금 지역 좌표 (기본값)
VITE_DEFAULT_LAT=35.7983
VITE_DEFAULT_LNG=126.7041

# 애플리케이션 메타데이터
VITE_APP_NAME="새만금 인사이트 대시보드"
VITE_APP_VERSION="1.0.0"
VITE_APP_DESCRIPTION="새만금개발청 공공데이터 기반 통합 대시보드"

# 외부 서비스 연동 (필요시)
# VITE_ANALYTICS_ID=your_analytics_id
# VITE_SENTRY_DSN=your_sentry_dsn